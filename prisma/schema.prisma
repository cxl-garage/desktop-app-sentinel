// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model ModelRun {
  id         Int     @id @default(autoincrement())
  modelName  String
  outputPath String
  startTime  Int
  inputPath String
  confidenceThreshold Float
  outputStyle String
}
